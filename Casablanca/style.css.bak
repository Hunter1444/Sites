.hamburger_checkbox{
	display:none
}
.top-menu__block{
	margin:0;
	padding:0;
	list-style-type:none;
	position:absolute;
	top:200px;
	left:-270px;
	height:300px;
	background-color:black;
	transform:translateX(0px);
	transition-duration:1s;
}
.top-menu__item{
	text-decoration:none;
	color:white;
	font-size:18px;
	padding:30px;
	line-height:50px;
}
.hamburger_checkbox:checked ~ .hamburger{
	transform: rotate(90deg);
}
.hamburger_checkbox:checked ~ .top-menu .top-menu__block{
	transform:translateX(250px);
	background-color:black;
}
.hamburger{
	float:left;
	clear:both;
	width:30px;
	height:24px;
	margin-left:0px;
	margin-top:60px;
	cursor:pointer;
	transition-duration:0.5s;
}
.hamburger__lines{
	display:block;
	width:30px;
	height:4px;
	background-color:white;
	position:relative;
}
.hamburger__lines::before,
.hamburger__lines::after{
	content:'';
	display:block;
	width:30px;
	position:absolute;
	height:4px;
	background-color:white;
}
.hamburger__lines::before{
	top:20px;
}
.hamburger__lines::after{
	top:10px;
}
